{
  "name": "@faiz-ui/react",
  "version": "1.1.5",
  "description": "ðŸš€ Beautiful and modern React UI library.",
  "license": "MIT",
  "keywords": [
    "react ui"
  ],
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "directory": "packages/core/react"
  },
  "scripts": {
    "build": "node ./src/scripts/prebuild.js && tsup --dts && node ./src/scripts/postbuild.js",
    "dev": "yarn build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "postbuild": "node ./src/scripts/postbuild.js",
    "prebuild": "node ./src/scripts/prebuild.js"
  },
  "dependencies": {
    "@faiz-ui/accordion": "workspace:*",
    "@faiz-ui/alert": "workspace:*",
    "@faiz-ui/avatar": "workspace:*",
    "@faiz-ui/button": "workspace:*",
    "@faiz-ui/landing-card": "workspace:*",
    "@faiz-ui/ripple": "workspace:*",
    "clean-package": "2.2.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "clean-package": "../../../clean-package.config.json"
}
